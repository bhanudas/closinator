<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Duration_Seconds__c</fullName>
    <description>Duration of batch execution in seconds</description>
    <externalId>false</externalId>
    <formula>IF(AND(NOT(ISBLANK(End_Time__c)), NOT(ISBLANK(Start_Time__c))), (End_Time__c - Start_Time__c) * 86400, null)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Duration (Seconds)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Number</type>
</CustomField>
